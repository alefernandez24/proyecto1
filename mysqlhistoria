CREATE DATABASE drupaldb;
#CREATE USER drupaluser@localhost IDENTIFIED BY 'password';
#CREATE USER drupaluser@localhost IDENTIFIED BY 'lolo';
CREATE USER drupaluser@localhost IDENTIFIED BY 'lolo';
GRANT SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,INDEX,ALTER,CREATE TEMPORARY TABLES,LOCK TABLES ON drupaldb.* TO drupaluser@localhost;
FLUSH PRIVILEGES;
show databases;
.mysql_history (END)
